package ch01;

import java.io.IOException;
import java.net.InetSocketAddress;

import com.sun.net.httpserver.*;

public class SimpleHttpServer {

	public static void main(String[] args) {
		
		try {
			HttpServer.create(new InetSocketAddress(8080), 0);
			
		} catch (IOException e) {
			e.printStackTrace();
		}
		 
	}
}

